{
  "name": "harvestbot",
  "version": "1.0.0",
  "description": "Chatbot for providing hours info from Harvest.",
  "main": "bin/index.js",
  "scripts": {
    "build": "rimraf build/ && babel ./src --out-dir build/ --ignore **/*.test.js",
    "ci:automerge": "circleci-automerge --filter=\"^greenkeeper/\"",
    "start": "npm run build && node ./build/local.js",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint ./src --format codeframe",
    "test:lint:fix": "eslint ./src --format codeframe --fix",
    "test:unit": "jest ./src --coverage"
  },
  "jest": {
    "testRegex": ".*\\.test\\.js$",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NewThingsCo/harvestbot"
  },
  "keywords": [
    "harvest",
    "tracking",
    "slack",
    "chatbot"
  ],
  "author": "NewThingsCo",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.3.4",
    "@google-cloud/datastore": "3.1.2",
    "@google-cloud/logging-winston": "0.11.0",
    "@google-cloud/pubsub": "0.25.0",
    "@google-cloud/storage": "2.4.3",
    "@sendgrid/mail": "6.3.1",
    "axios": "0.18.0",
    "googleapis": "38.0.0",
    "rxjs": "6.4.0",
    "winston": "3.2.1",
    "xlsx": "0.14.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@babel/plugin-transform-runtime": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "babel-core": "7.0.0-bridge.0",
    "circleci-automerge": "1.1.0",
    "commander": "2.19.0",
    "config": "3.0.1",
    "eslint": "5.15.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "jest": "24.1.0",
    "rimraf": "2.6.3"
  }
}
